{
  "name": "airbitz-cli",
  "version": "0.3.2",
  "description": "Airbitz CLI tool",
  "homepage": "https://airbitz.co",
  "repository": {
    "type": "git",
    "url": "git@github.com:Airbitz/airbitz-cli.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Airbitz, Inc.",
  "contributors": [
    "Paul Puey <paul@airbitz.co>",
    "William Swanson <william@airbitz.co>",
    "Tim Horton <tim@airbitz.co>"
  ],
  "files": [
    "bin/*",
    "changelog.md",
    "lib/*",
    "LICENSE",
    "package.json",
    "readme.md"
  ],
  "main": "lib/airbitz-cli.cjs.js",
  "module": "lib/airbitz-cli.es.js",
  "bin": "bin/airbitz-cli",
  "scripts": {
    "build": "rollup -c && rollup -c rollup.config.node.js",
    "format": "import-sort -l --write '*.js' 'src/**/*.js'; prettier-eslint --write '*.js' 'src/**/*.js'",
    "lint": "eslint '*.js' 'src/**/*.js'",
    "precommit": "lint-staged && npm run build",
    "prepare": "npm run lint && npm run build",
    "updot": "updot"
  },
  "lint-staged": {
    "*.js": [
      "import-sort --list-different",
      "prettier-eslint --list-different"
    ]
  },
  "dependencies": {
    "base-x": "^2.0.0",
    "chalk": "^1.1.3",
    "edge-core-js": "^0.15.10",
    "edge-exchange-plugins": "^0.1.1",
    "hash.js": "^1.0.3",
    "lib-cmdparse": "^0.1.0",
    "node-getopt": "^0.2.3",
    "rfc4648": "^0.9.1",
    "source-map-support": "^0.4.14",
    "utf8": "^3.0.0",
    "xdg-basedir": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.1.1",
    "husky": "^0.14.3",
    "import-sort-cli": "^4.2.0",
    "import-sort-parser-babylon": "^4.2.0",
    "import-sort-style-module": "^4.2.0",
    "lint-staged": "^6.0.0",
    "prettier-eslint-cli": "^4.7.0",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "updot": "^1.0.3"
  },
  "importSort": {
    ".js, .es": {
      "parser": "babylon",
      "style": "module"
    }
  }
}
