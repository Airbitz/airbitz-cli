{
  "name": "airbitz-cli",
  "version": "0.4.1",
  "description": "Airbitz CLI tool",
  "homepage": "https://airbitz.co",
  "repository": {
    "type": "git",
    "url": "git@github.com:Airbitz/airbitz-cli.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Airbitz, Inc.",
  "contributors": [
    "Paul Puey <paul@airbitz.co>",
    "William Swanson <william@airbitz.co>",
    "Tim Horton <tim@airbitz.co>"
  ],
  "files": [
    "bin/*",
    "CHANGELOG.md",
    "lib/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "main": "lib/airbitz-cli.cjs.js",
  "module": "lib/airbitz-cli.js",
  "bin": "bin/airbitz-cli",
  "scripts": {
    "build": "rollup -c",
    "format": "import-sort -l --write '*.js' 'src/**/*.js'; npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore .",
    "precommit": "lint-staged && npm run build",
    "prepare": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.js": [
      "import-sort --list-different",
      "eslint"
    ]
  },
  "dependencies": {
    "base-x": "^2.0.0",
    "chalk": "^1.1.3",
    "edge-core-js": "^0.15.10",
    "edge-exchange-plugins": "^0.6.0",
    "hash.js": "^1.0.3",
    "lib-cmdparse": "^0.1.0",
    "node-getopt": "^0.2.3",
    "rfc4648": "^0.9.1",
    "source-map-support": "^0.4.14",
    "utf8": "^3.0.0",
    "xdg-basedir": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "eslint": "^6.0.0",
    "eslint-config-standard-kit": "^0.14.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.7",
    "import-sort-cli": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^6.0.0",
    "prettier": "^1.18.2",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.0.0"
  },
  "importSort": {
    ".js, .es": {
      "style": "module"
    }
  }
}
